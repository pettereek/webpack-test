{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "A simple webpack/React/jest setup",
  "main": "dist/index.html",
  "scripts": {
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "jest-cli": "^0.4.0",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "webpack": "^1.12.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
